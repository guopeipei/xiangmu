body
  width 100%
  header
    width 100%
    background #89be48
    position fixed
    top 0
    padding 0
    margin auto
    z-index 999
    .header-content
      width 100%
      margin 0 auto
      height 48px
      .btn-back
        top 8px
        .leftback-img
          background url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAeAQMAAAAW3KwoAAAABlBMVEUAAAD///+l2Z/dAAAAAXRSTlMAQObYZgAAADZJREFUCNdjYGA4wMDACMTMDQwM7EAuHxDLALEFEBcA8QMc0lAsAxYDyYHUgNSC9ID04lNwAACHIArVHBwTzAAAAABJRU5ErkJggg==')no-repeat
          display block
          width 12px
          height 45px
          position absolute
          top 8px
          left 4%
      .header-title
        margin 0 auto
        line-height 48px
        font-size 24px
        text-align center
        color #fff
        width 80%
        white-space nowrap
        text-overflow ellipsis
        overflow hidden
      .btn-back
        width 10%
        height 46px
        position absolute
        left 0
        top 0


    .header-login-wrap
      width 100%
      height 40px
      background #f0fae6
      margin 0
      padding 10px 0 6px
      .header-login
        width 74%
        min-height 34px
        margin 0 auto
        padding 0 0 0 21%
        position relative
        .btn-login
          width 18%
          height 32px
          background #fff
          border 1px solid #8bc146
          text-align center
          font-size 18px
          color #88c13e
          line-height 30px
          margin 0
          display block
          position absolute
          left 0
          top 0
        .login-tip
          width 100%
          text-align left
          vertical-align middle
          font-size 16px
          color #333333
          line-height 18px

.wrap-content
  margin-top 100px
  background #f0f0f0
  .header-exchange-wrap
    width 100%
    height 38px
    background-color #fff
    margin 0
    padding 8px 0 6px
    border-bottom 1px solid #f2f2f2
    .header-exchange
      width 93%
      height 36px
      margin 1px auto 0
      position relative
      padding 0
      .exch-ipt
        padding-left 10px
        width 80%
        height 32px
        line-height 32px
        display inline-block
        border 1px solid #e7e7e7
        background #fff
        font-size 16px
        text-align center
        outline 0 none
      .exch-btn
        width 20%
        height 36px
        line-height 36px
        display inline-block
        background #fff
        text-align center
        font-size 18px
        color #6fbd00
        letter-spacing 2px
        border 1px solid #6fbd00
        position absolute
        right 0
        top 0


  .wrap-nodata
    margin 0 auto
    .carts-img
      margin 0 auto
      text-align center
      padding-left 24px
      padding-top 10px
      box-sizing border-box
      img
        max-width 160px
        vertical-align middle
    .carts-text
      text-align center
      font-size 20px
      color rgb(102,102,102)
      line-height 36px
    .carts-link
      text-align center
      a
        border 1px solid rgb(137, 190, 72)
        padding 6px 18px
        display inline-block
        border-radius 6px
        font-size 18px
        letter-spacing 1px
        margin-top 6px
        color rgb(137, 190, 72)
    .wrap-cart-assem
      width 100%
      min-width 320px
      max-width 640px
      background-color #fff
      border-bottom 1px solid #f2f2f2
      margin 12px auto 0
      .cart-assem-title
        width 94%
        border-bottom 1px solid #ccc
        text-align left
        font-size 18px
        color #333333
        line-height 24px
        margin 0 auto
        padding 10px 3%
      .cart-assem-list
        width 100%
        max-height 224px
        margin 0 auto
        padding 16px 0
        overflow auto
        ul
          height 144px
          width 2000px
          li
            width 96px
            display inline-block
            vertical-align top
            text-align center
            margin 0 5px
            .cart-assem-item
              width 100%
              a
                color #333
                img
                  width 100%
                  display block
                  vertical-align middle
                  max-width 100%
              .title
                width 100%
                text-align left
                font-size 14px
                color #333333
                line-height 24px
                font-weight 400
                text-overflow ellipsis
                white-space nowrap
                overflow hidden
              .price
                font-size 14px
                text-align left
                line-height 24px



